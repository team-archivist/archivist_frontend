{
  "eslint.options": {
    "configFile": "./.eslintrc.js"
  },
  "editor.fontSize": 14,
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": true
  },
  "editor.tabSize": 2,
  "eslint.alwaysShowStatus": true,
  "editor.formatOnPaste": true,
  "editor.formatOnSave": true,
  "editor.inlineSuggest.enabled": true,
}